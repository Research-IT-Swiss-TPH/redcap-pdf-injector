{
    "name": "PDF Injector",

    "namespace": "STPH\\pdfInjector", 

    "description": "Fill form fields of your PDFs with your redcap data.",

    "documentation": "README.md",

    "authors": [
        {
             "name": "Ekin Tertemiz",
             "email": "ekin.tertemiz@swisstph.ch",
             "institution": "Swiss Tropical and Public Health Institute"
         }
     ],

    "framework-version": 6,

    "permissions": [
        "redcap_every_page_top"
    ],

    "enable-every-page-hooks-on-system-pages": false,

    "links":{
        "project": [                 
            {
                "name": "PDF Injector",
                "key": "pdf-injector",
                "icon": "fas fa-syringe",
                "url": "index.php",
                "show-header-and-footer": true
            }
        ]
    },

    "no-auth-pages": [],

    "system-settings": [],

    "project-settings": [
        {
            "key": "javascript-debug",
            "name": "Output debug information to the browser console",
            "type": "checkbox"
        },        
        {
            "key": "pdf-injections",
            "name": "PDF Injections",
            "type": "json",
            "hidden": true
        },
        {
            "key": "last-injection-id",
            "hidden": true
        }
    ],

    "crons": [],

    "compatibility": {
        "php-version-min": "5.4.0",
        "redcap-version-min": "8.0.0"
    }
 }